# Environment-specific configuration for staging
# This file contains variables that are specific to the staging environment

# Project Configuration
STAGING_PROJECT_ID=staging-adk
ENVIRONMENT=staging
REGION=us-central1

# Cloud Run Configuration
SERVICE_NAME=my-agentic-rag
MEMORY=2Gi
CPU=2
MAX_INSTANCES=50
MIN_INSTANCES=0
CONCURRENCY=80

# Discovery Engine Configuration
DATA_STORE_ID=my-agentic-rag-datastore
DATA_STORE_REGION=us

# Data Pipeline Configuration
PIPELINE_ROOT=gs://staging-adk-my-agentic-rag-rag
PIPELINE_SA_EMAIL=my-agentic-rag-rag@staging-adk.iam.gserviceaccount.com
PIPELINE_NAME=data-ingestion-pipeline
PIPELINE_CRON_SCHEDULE="0 2 * * *"  # Daily at 2 AM
DISABLE_CACHING=FALSE

# Container Registry Configuration
ARTIFACT_REGISTRY_REPO=my-agentic-rag-docker-repo
CONTAINER_NAME=my-agentic-rag

# Logging Configuration
LOGS_BUCKET=gs://staging-adk-my-agentic-rag-logs-data/build-logs
LOAD_TEST_RESULTS_BUCKET=staging-adk-my-agentic-rag-load-test-results

# Security Configuration
ALLOW_UNAUTHENTICATED=true
IAP_ENABLED=false

# Monitoring Configuration
ENABLE_TRACING=true
ENABLE_LOGGING=true
LOG_LEVEL=INFO

# Load Testing Configuration
LOAD_TEST_DURATION=30s
LOAD_TEST_USERS=10
LOAD_TEST_SPAWN_RATE=0.5